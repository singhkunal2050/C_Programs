/* CHECK WHETHER A CHARACTER IS DIGIT OR ALPHABET */

#include<stdio.h>
main()
{
int a;
printf("ENTER A CHAR\n");
scanf("%d",&a);
if(a>=1 && a<=10)
{
printf("THE CHAR IS A DIGIT  %d  & IT LIES B|W 1 to 10\n",a);
}
else
{
printf("THE CHAR ISNT A DIGIT\n");
}
}
/*
OUTPUT

ENTER A CHAR
a
THE CHAR ISNT A DIGIT
*/
