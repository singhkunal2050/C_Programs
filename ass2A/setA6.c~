/* CHECK VOWELS*/
#include <stdio.h>
main()
{
char a;
printf("ENTER A CHARACTER TO CHECK IF ITS A VOWEL \n");
scanf("%c",&a);
if(a=='a' || a=='A')
{
 printf("%c IS A VOWEL \n",a);
} 
else
if(a=='e' || a=='E')
{
 printf("%c IS A VOWEL \n",a);
} 
else
if(a=='i' || a=='I')
{
 printf("%c IS A VOWEL \n",a);
} 
else
if(a=='o' || a=='O')
{
 printf("%c IS A VOWEL \n",a);
} 
else
if(a=='u' || a=='U')
{
 printf("%c IS A VOWEL \n",a);
} 
else
{
 printf("%c IS'NT A VOWEL \n",a);
} 

}


/*
OUTPUT 

ENTER A CHARACTER TO CHECK IF ITS A VOWEL 
a
a IS A VOWEL 


*/
