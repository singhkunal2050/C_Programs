	/* CREATING FUNCTION FOR N EVEN NUMBERS */

	#include<stdio.h>

	 int IsEven(int n)
	{

	if(n%2==0)  return 1; 
	else return 0;
	}


	main()
	{
	int f,i,no;
	
		printf("ENTER HOW MANY NOS U HAVE TO CHECK \n");
		scanf("%d",&f);

	for(i=1;i<=f;i++)

	{
		printf(" \nENTER NUM \n ");
		scanf("%d",&no);
	if(IsEven(no))
		printf(" %d  IS EVEN\t",no);
	else;
	

	}
	}


/*
OUTPUT

ENTER HOW MANY NOS U HAVE TO CHECK 
5
ENTER NUM 
 223
ENTER NUM 
 23
ENTER NUM 
 54
 54  IS EVEN    
ENTER NUM 
 34
 34  IS EVEN
ENTER NUM 
 2 
 2  IS EVEN    

*/

