/*ARMSTRONG NOS B|W 1 to 500 */
#include<stdio.h>
main()
{

	int d,t,sum,nt;
	printf(" \n THE ARMSTRONG NUMBER B|W 1 to 500 IS \n");
	for(nt=1;nt<=500;nt++)
	{
		sum=0;
		t=nt; 
		while(t!=0)
		{
			d=t%10;
			sum=sum+(d*d*d);		
			t=t/10;
		}
		if(nt==sum)
			printf("%d\t" ,nt);
	}
}
/*OUTPUT

 THE ARMSTRONG NUMBER B|W 1 to 500 IS 
1       153     370     371     407  

*/


